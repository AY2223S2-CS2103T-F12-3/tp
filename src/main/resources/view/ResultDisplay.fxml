<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ScrollPane?>
<StackPane fx:id="placeHolder" styleClass="pane-with-rounded-corner"
           xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <VBox>
    <Label text="Command result" styleClass="pa, label-h6" />
    <Separator />
    <ScrollPane fitToWidth="true" hbarPolicy="NEVER" styleClass="result-display, p0">
      <VBox fx:id="resultDisplayContainer" spacing="10" styleClass="result-display, pa" />
    </ScrollPane>
  </VBox>
</StackPane>
